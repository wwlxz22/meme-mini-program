@import "../../../common/common.wxss";

page {
  background: #f2f2f2;
}

.feedback-item {
  width: 700rpx;
  margin-top: 25rpx;
  margin-left: 25rpx;
  background: #fff;
  padding-bottom: 30rpx;
}

.feedback-title {
  margin-top: 26rpx;
  margin-left: 30rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #333;
  text-align: justify;
  line-height: 32rpx;
}

.feedback-input {
  padding-top: 30rpx;
  padding-left: 30rpx;
}

.placeholder {
  width: 320rpx;
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #999;
  text-align: justify;
  line-height: 32rpx;
}

.submit {
  margin-top: 60rpx;
  background: red;
  font-family: PingFangSC-Regular;
  font-size: 18px;
  color: white;
  text-align: center;
  width: 700rpx;
  height: 88rpx;
}
